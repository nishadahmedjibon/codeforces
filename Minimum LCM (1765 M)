#include<bits/stdc++.h>
typedef long long int ll;
using namespace std;
 
 
 
void weirdo()
{
    int n;
    cin >> n;
    int a = 1;
    for (int i = 2; i * i <= n; ++i) {
      if (n % i == 0) {
        a = n / i;
        break;
      }
    }
    cout << a << ' ' << n - a << '\n';
 
}
int main()
{
    int t; cin>>t;
    while(t--)
    {
        weirdo();
    }
}
