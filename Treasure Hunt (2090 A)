#include<bits/stdc++.h>
typedef long long int ll;
using namespace std;

void weirdo()
{
    ll x, y, a;
    cin>>x>>y>>a;
    ll b=x+y, d=a*5;
    ll c=d/b;
    ll e=(c*b) +x;
    if(e>=d)
        cout<<"NO\n";
    else
        cout<<"YES\n";
}
int main()
{
    int t; cin>>t;
    while(t--)
    {
        weirdo();
    }
}
