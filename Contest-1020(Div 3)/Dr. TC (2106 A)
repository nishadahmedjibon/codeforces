#include<bits/stdc++.h>
typedef long long int ll;
#define nl '\n';
using namespace std;
 
void weirdo()
{
    int n;
    cin>>n;
    string s;
    cin>>s;
    int cnt1=0, cnt=0;
    for(int i=0; i<n; i++)
    {
        if(s[i]=='0')
            cnt++;
        else
            cnt1++;
    }
    cout<<(n*cnt1)-cnt1+cnt<<endl;
}
int main()
{
    int t; cin>>t;
    while(t--)
    {
        weirdo();
    }
}
